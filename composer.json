{
    "name": "angelina/hackathon-app",
    "description": "Hackathon registration system with GitHub API integration",
    "type": "project",
    "keywords": ["hackathon", "api", "github", "php", "docker"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lyubanskaya Angelina",
            "email": "chumachenkoangelina@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "ext-gd": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "www/"
        },
        "files": [
            "www/ApiClient.php",
            "www/UserInfo.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "echo 'Autoload dump complete'"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}